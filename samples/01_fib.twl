# Twilight Programming Language

module Fib (

  import print from "twilight/core"
  import to_string from "twilight/core"

  class Main (

    def main():
      print(to_string(this.fib(40)))

    def fib(x:Number):
      when: (
        x < 3:  1
         else:  fib(x - 1) + fib(x-2)
      )

  )
);
